<!doctype html>
<html lang="{{language}}">
  <head>
<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1.0, shrink-to-fit=no">
<meta name="description" content="Race Horology Documentation">
<meta name="author" content="Race Horology Development Team">

<link href="{{ '/assets/images/favicon.png' | url }}" rel="icon" />

<!-- Stylesheet -->
<!-- Bootstrap -->
<link rel="stylesheet" type="text/css" href="{{ '/assets/vendor/bootstrap/css/bootstrap.min.css' | url }}" />
<!-- Font Awesome Icon -->
<link rel="stylesheet" type="text/css" href="{{ '/assets/vendor/font-awesome/css/all.min.css' | url }}" />
<!-- Magnific Popup -->
<link rel="stylesheet" type="text/css" href="{{ '/assets/vendor/magnific-popup/magnific-popup.min.css' | url }}" />
<!-- Highlight Syntax -->
<link rel="stylesheet" type="text/css" href="{{ '/assets/vendor/highlight.js/styles/github.css' | url }}" />
<!-- Custom Stylesheet -->
<link rel="stylesheet" type="text/css" href="{{ '/assets/css/stylesheet.css' | url }}" />

<title>{{ title }}</title>
  </head>

  <body data-spy="scroll" data-target=".idocs-navigation" data-offset="125">
  <!-- Preloader -->
    <div class="preloader">
    <div class="lds-ellipsis">
        <div></div>
        <div></div>
        <div></div>
        <div></div>
    </div>
    </div>
    <!-- Preloader End --> 

<!-- Document Wrapper   
=============================== -->
<div id="main-wrapper"> 
  
  {% include "layouts/_base/header.njk" %}
  
  <!-- Content
  ============================ -->
  <div id="content" role="main">
    
    {% include "layouts/_base/sidebarnavigation.njk" %}
    
    <!-- Docs Content
	============================ -->
    <div class="idocs-content">
      <div class="container"> 
        
        <section id="idocs_start">

        {{ content | safe }}

        </section>
        
      </div>
    </div>
	
  </div>
  <!-- Content end --> 
  
{% include "layouts/_base/footer.njk" %}

</div>
<!-- Document Wrapper end --> 

<!-- Back To Top --> 
<a id="back-to-top" data-toggle="tooltip" title="Back to Top" href="javascript:void(0)"><i class="fa fa-chevron-up"></i></a> 

<!-- JavaScript
============================ -->
<script src="{{ '/assets/vendor/jquery/jquery.min.js' | url }}"></script> 
<script src="{{ '/assets/vendor/bootstrap/js/bootstrap.bundle.min.js' | url }}"></script> 
<!-- Highlight JS -->
<script src="{{ '/assets/vendor/highlight.js/highlight.min.js' | url }}"></script> 
<!-- Easing --> 
<script src="{{ '/assets/vendor/jquery.easing/jquery.easing.min.js' | url }}"></script> 
<!-- Magnific Popup --> 
<script src="{{ '/assets/vendor/magnific-popup/jquery.magnific-popup.min.js' | url }}"></script> 
<!-- Custom Script -->
<script src="{{ '/assets/js/theme.js' | url }}"></script>
</body>
</html>